<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.step_trackertest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Step Tracker"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity1" />
        <activity android:name=".activity2" />
        <activity android:name=".activity3" />
        <activity android:name=".activity4" />
        <activity android:name=".naviActivity" />
        <activity android:name=".SensorFusionMath" />
        <activity android:name=".SimpleStepDetector" />
        <activity android:name=".StepListener" />


        <service
            android:name=".MyIntentService"
            android:exported="false"></service>
    </application>

</manifest>
